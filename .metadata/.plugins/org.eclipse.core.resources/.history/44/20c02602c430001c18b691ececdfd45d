package com.cjc.app.evf.main.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class EMIDetails {
	@Id
	public int emiId;
	public Double emiMonthlyPaid;
	public String emiNextDueDate;
	public String previousEmiStatus;
	
	
	public int getEmiId() {
		return emiId;
	}
	public void setEmiId(int emiId) {
		this.emiId = emiId;
	}
	public Double getEmiMonthlyPaid() {
		return emiMonthlyPaid;
	}
	public void setEmiMonthlyPaid(Double emiMonthlyPaid) {
		this.emiMonthlyPaid = emiMonthlyPaid;
	}
	public String getEmiNextDueDate() {
		return emiNextDueDate;
	}
	public void setEmiNextDueDate(String emiNextDueDate) {
		this.emiNextDueDate = emiNextDueDate;
	}
	public String getPreviousEmiStatus() {
		return previousEmiStatus;
	}
	public void setPreviousEmiStatus(String previousEmiStatus) {
		this.previousEmiStatus = previousEmiStatus;
	}
	

}
